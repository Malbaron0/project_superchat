<div class="all-content">
    <div class="chat-header">
        <div class="content">
            <h1>SUPER CHAT</h1>
        </div>
    </div>

    <div class="chat-body">
        <div class="chat-rooms">
             {{!-- {{#each chatRoom}}  --}}
                <div class="room">
                    <h3>test</h3>
                     {{#each messagesArr}} 
                    <ul>
                         <li>{{this.body}} and {{this.author}}</li> 
                    </ul>
                     {{/each}} 
                </div>

             {{!-- {{/each}}  --}}
        </div>
    </div>

    <div class="chat-form">
        <div class="inputs">
                <input placeholder="Enter message here" name="messageInput" id="messageInput">
                <button id="submit">Post</button>
        </div>
    </div>



    </div>
</div>